<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:appepi="clr-namespace:AppEpi.ViewModels"
             Title="Recebimento da Higienização"
             x:Class="AppEpi.Views.RecebimentoHigienizacao">
  <ContentPage.Content>
    <StackLayout Margin="10,10" Spacing="10">
      
      <Label>EPIs</Label>
      
      <appepi:EpcListView x:Name="epcList" />
      
      <Button 
        x:Name="btnEnvioTeste"
        Text="Confirmar" 
        FontSize="18" 
        Clicked="btnEnvioTeste_Clicked"
        Style="{StaticResource BotaoAzul1}"/>
   
    </StackLayout>
  </ContentPage.Content>
</ContentPage>